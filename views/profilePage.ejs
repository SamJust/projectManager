<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <link rel="stylesheet" type="text/css" href="/public/navigation.css">
  <link rel="stylesheet" type="text/css" href="/public/profilePage.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
</head>
<body>
  <div>
    <%- include('partials/nav.ejs', {user:userInfo}) %>
  </div>
  <main>

    <ul>
      <li>Login:<%=userInfo.username%></li>
      <li>email:<%=email%></li>
    </ul>
    <br>
    <h2>Projects:</h2>

    <ul>
      <%for(let i = 0; i < userInfo.projectsArray.length; i++){%>
        <li><a href="project/<%=userInfo.projectsArray[i].id%>"><%=userInfo.projectsArray[i].name%></a></li>
        <%}%>
    </ul>

    <div id="projectOptions">
      <a class="projectOptionsButtons" href="newproject">Create new project</a>
    </div>
  </main>
</body>
</html>
